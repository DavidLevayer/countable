{
  "name": "countable",
  "version": "0.1.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"npm run front\" \"npm run server\"",
    "build": "npm run clean && npm run build-server && ng build",
    "build-server": "tsc -p server",
    "front": "ng serve",
    "server": "export NODE_ENV=dev && npm run build-server && node dist/server/app.js",
    "lint": "tslint \"src/**/*.ts\" \"server/**/*.ts\"",
    "back-test": "export NODE_ENV=test && npm run build-server && mocha dist/server/**/*.spec.js",
    "front-test": "ng test --watch=false --code-coverage",
    "test": "npm run back-test && npm run front-test",
    "clean": "rm -rf dist coverage && npm cache clean",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^2.3.1",
    "@angular/compiler": "^2.3.1",
    "@angular/core": "^2.3.1",
    "@angular/forms": "^2.3.1",
    "@angular/http": "^2.3.1",
    "@angular/platform-browser": "^2.3.1",
    "@angular/platform-browser-dynamic": "^2.3.1",
    "@angular/router": "^3.3.1",
    "body-parser": "^1.15.2",
    "bootstrap": "^4.0.0-alpha.6",
    "core-js": "^2.4.1",
    "express": "^4.14.0",
    "multer": "^1.3.0",
    "rxjs": "^5.0.1",
    "sqlite3": "^3.1.8",
    "ts-express-decorators": "1.4.0",
    "ts-helpers": "^1.1.1",
    "ts-httpexceptions": "^2.1.3",
    "zone.js": "^0.7.2"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0-beta.33.1",
    "@angular/compiler-cli": "^2.3.1",
    "@types/body-parser": "0.0.33",
    "@types/chai": "^3.4.34",
    "@types/chai-http": "0.0.29",
    "@types/express": "^4.0.34",
    "@types/jasmine": "2.5.38",
    "@types/mocha": "^2.2.35",
    "@types/multer": "0.0.33",
    "@types/node": "^6.0.42",
    "@types/sqlite3": "^2.2.31",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "codelyzer": "~2.0.0-beta.1",
    "concurrently": "^3.1.0",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-cli": "1.0.1",
    "karma-jasmine": "1.0.2",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-remap-istanbul": "0.2.1",
    "mocha": "^3.2.0",
    "mocha-typescript": "^1.0.15",
    "protractor": "~4.0.13",
    "ts-node": "^1.2.1",
    "tslint": "^4.0.2",
    "typescript": "~2.0.3"
  }
}
